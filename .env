# ---- Auth ----
HUGGING_FACE_HUB_TOKEN=placeholder

# ---- vLLM - Chat (LLM behavior) ----
VLLM_CHAT_MODEL=Qwen/Qwen2.5-3B-Instruct
VLLM_CHAT_DTYPE=auto
VLLM_CHAT_GPU_UTILIZATION=0.75

# ---- vLLM - Embeddings (embedder behavior) ----
VLLM_EMBED_MODEL=nomic-ai/nomic-embed-text-v1
VLLM_EMBED_DTYPE=auto
VLLM_EMBED_GPU_UTILIZATION=0.50

# ---- MinIO creds (keep in .env so you don't commit secrets) ----
MINIO_ROOT_USER=placeholder
MINIO_ROOT_PASSWORD=placeholder

# ---- Optional: tweak without editing YAML ----
QDRANT_URI=http://qdrant:6333
WEBUI_NAME=Adam's Local LLM


